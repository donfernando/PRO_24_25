<root xmlns:nsd="https://structorizer.fisch.lu" version="3.32-22" preRepeat="until " postFor="to" preReturn="return" postForIn="in" preWhile="while " output="OUTPUT" input="INPUT" preFor="for" preExit="exit" preLeave="leave" ignoreCase="true" preThrow="throw" preForIn="foreach" stepFor="by" author="1dawa" created="2024-09-30" changedby="ftoblare" changed="2024-10-01" origin="Structorizer 3.32-22" text="&#34;Dado un número de segundos determinado por teclado, mostrar su correspondencia en horas, minutos y segundos.&#34;" comment="" color="ffffff" type="program" style="nice">
	<children>
		<instruction text="&#34;INPUT segundosTotales&#34;,&#34;segundos &#60;- segundosTotales&#34;" comment="&#34;Leer los segundos totales del usuario&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
